<UserControl
  x:Class="DirN.Views.PointerControl.PEnum"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
  xmlns:local="clr-namespace:DirN.Views.PointerControl"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  xmlns:pointercontrol="clr-namespace:DirN.ViewModels.PointerControl"
  d:DataContext="{d:DesignInstance Type=pointercontrol:PEnumViewModel}"
  d:DesignHeight="450"
  d:DesignWidth="800"
  mc:Ignorable="d">
  <Grid>
    <ComboBox
      Grid.Column="1"
      Padding="3"
      ItemsSource="{Binding EnumValues}"
      SelectedItem="{Binding SelectedItem}" />
  </Grid>
</UserControl>
